@model VehicleViewModel
@using Web.Models

<form asp-action="EditUser" role="form">
    @await Html.PartialAsync("_ModalHeader", new ModalHeader { Heading = "Edit Parking Lot" })
    <div class="modal-body form-horizontal">
        <div class="row">
                <div class="form-group">
                <label asp-for="LicensePlate" class="col-lg-3 col-sm-3 control-label"></label>
                <div class="col-lg-6">
                    <input asp-for="Brand" class="form-control" />
                </div>
            </div>
            <div class="form-group">
                <label asp-for="Model" class="col-lg-3 col-sm-3 control-label"></label>
                <div class="col-lg-6">
                    <input asp-for="Model" class="form-control" />
                </div>
            </div>
            <div class="form-group">
                <label asp-for="ParkingSpotId" class="col-lg-3 col-sm-3 control-label"></label>
                <div class="col-lg-6">
                    <input asp-for="ParkingSpotId" class="form-control" />
                </div>
            </div>
        </div>
    </div>
    @await Html.PartialAsync("_ModalFooter", new ModalFooter { })
</form>  